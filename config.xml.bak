   <Menu>
      <Name>Menu</Name>
      <Children>

	 <LaunchMenu>
	    <Name>MAME</Name>
      <Directory>./mame4all/</Directory>
	    <Target>mame4all</Target>
	    <StatusText>MAME4ALL</StatusText>
	 </LaunchMenu>

	 <LaunchFileMenu>
	    <Name>NES</Name>
      <Directory>./roms/nes</Directory>
	    <StatusText>Nintendo Entertainment System (1983)</StatusText>
	 </LaunchFileMenu>

   <Action>
     <Name>Reboot</Name>
     <StatusText>Reboot Pi</StatusText>
		 <Action>Reboot</Action>
   </Action>

	 <Action>
	   <Name>Shutdown</Name>
 	   <StatusText>Shutdown Pi</StatusText>
	   <Action>Shutdown</Action>
   </Action>

      </Children>
   </Menu>

</Environment>
